{
  "name": "express-mongoose-typescript",
  "version": "1.7.0",
  "description": "Just Boilerplate Express, Mongoose, and TypeScript ( ðŸ¦¡ ) ",
  "private": true,
  "scripts": {
    "dev": "npm run start:babel",
    "start": "nodemon ./src/bin/server.ts",
    "start:babel": "nodemon --exec babel-node --extensions \".ts\" ./src/bin/server.ts",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "refresh:env-jwt": "node ./src/scripts/generateEnvJWT.ts",
    "clean": "rm -rf ./dist",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "before-build": "yarn && npm run lint && npm run type-check && npm run clean",
    "build": "npm run before-build && tsc -p .",
    "build:babel": "npm run before-build && babel src --out-dir dist --extensions \".ts\",\".js\"",
    "serve:staging": "NODE_ENV=staging node ./dist/bin/server.js",
    "serve:production": "NODE_ENV=production node ./dist/bin/server.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/bin/server.js",
    "mongo:restore": "mongorestore dump/",
    "mongo:dump": "mongodump --db=${db} --out dump/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Pre-commit checks...'",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint"
    }
  },
  "author": "masb0ymas",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "debug": "~4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "googleapis": "^61.0.0",
    "handlebars": "^4.7.6",
    "helmet": "^4.1.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.10",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.0",
    "redis": "^3.0.2",
    "swagger-jsdoc": "^4.3.1",
    "swagger-ui-express": "^4.1.4",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.162",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.2",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.28",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.x"
  }
}
